{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
  "rules": {
    "quotes": ["warn", "single"],
    "indent": ["warn", 2],
    "no-multi-spaces": [
      "warn",
      {
        "ignoreEOLComments": false
      }
    ],
    "no-trailing-spaces": "warn",
    "semi": ["warn", "never"]
  }
}
